{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "customerName": {
      "type": "string",
      "required": true
    },
    "tableName": {
      "type": "string"
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "served",
        "paid",
        "cancelled"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "fixedDiscount": {
      "type": "decimal"
    },
    "percentualDiscount": {
      "type": "integer"
    },
    "totalTaxes": {
      "type": "decimal"
    },
    "finalTotal": {
      "type": "decimal"
    },
    "notes": {
      "type": "text"
    },
    "order_rows": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-row.order-row"
    },
    "serial": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "paidAmount": {
      "type": "string"
    },
    "appliedDiscount": {
      "type": "string"
    },
    "paymentDaytime": {
      "type": "datetime"
    },
    "paymentMethod": {
      "type": "string"
    }
  }
}
